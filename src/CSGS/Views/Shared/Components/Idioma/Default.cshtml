@model List<SelectListItem>

<form asp-action="CambiarIdioma"
      asp-controller="Home"
      asp-route-returnUrl="@Context.Request.Path">
    <select name="culture"
            asp-items="@(new SelectList(Model,"Value","Text",ViewBag.IdiomaSel))"></select>
    <button class="btn btn-primary">
        Cambiar
    </button>
</form>